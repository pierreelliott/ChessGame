<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="218.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
         <top>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Partie finie" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Berlin Sans FB Bold" size="39.0" />
               </font>
            </Label>
         </top>
   <center>
      <AnchorPane fx:id="anchorID" BorderPane.alignment="CENTER">
         <children>
                  <GridPane fx:id="gridID" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
               <children>
                  <Button fx:id="exitButton" mnemonicParsing="false" onAction="#btnClicked" text="OK" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                     <padding>
                        <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
                     </padding>
                  </Button>
                  <Label fx:id="labelID" text="" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
               </children>
                  </GridPane>
         </children>
      </AnchorPane>
   </center>
      </BorderPane>
